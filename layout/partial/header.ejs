      <ul class="nav navbar-nav">
		 <% site.categories.forEach(function(category){%>
			<% site.posts.forEach(function(post){ %>
				<% post.categories.forEach(function(postcategory){ %>
					<% if(postcategory.name == category.name){ %>
						<li><a href="<%- post.path %>"><%- category.name %></a></li>
						<% return %>
					<% } %>
				<% )} %>	
		    <% }) %>
	    <% }) %>
      </ul>
    </div>
